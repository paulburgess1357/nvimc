{
  "default": true,
  "line-length": {
    "line_length": 140,
    "heading_line_length": 160,
    "code_block_line_length": 160
  },
  "no-duplicate-heading": {
    "siblings_only": true
  },
  "no-trailing-punctuation": {
    "punctuation": ".,;:"
  },
  "no-inline-html": false,
  "no-bare-urls": false,
  "first-line-heading": false,
  "no-multiple-blanks": {
    "maximum": 2
  }
}